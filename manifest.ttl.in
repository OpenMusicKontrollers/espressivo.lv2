@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix owl:	<http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ui:   <http://lv2plug.in/ns/extensions/ui#> .

@prefix esp:	<http://open-music-kontrollers.ch/lv2/espressivo#> .

esp:tuio2_in
	a lv2:Plugin ;
	lv2:minorVersion @ESPRESSIVO_MINOR_VERSION@ ;
	lv2:microVersion @ESPRESSIVO_MICRO_VERSION@ ;
	lv2:binary <espressivo@CMAKE_SHARED_MODULE_SUFFIX@> ;
	rdfs:seeAlso <espressivo.ttl> .

